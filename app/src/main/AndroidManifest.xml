<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.onmobile.gamification.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GamificationDemo">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.onmobile.sdk.gamification.GamificationPage"
            android:theme="@style/Theme_Compact" />
        <activity
            android:name="com.onmobile.sdk.gamification.GamificationTranslucentPage"
            android:theme="@style/Theme_Translucent" />
        <activity
            android:name="com.onmobile.sdk.gamification.GamificationTranslucentDialog"
            android:theme="@style/Theme_Translucent" />
    </application>

</manifest>